{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Lab5 (all)",
      "type": "shell",
      "command": "C:/msys64/ucrt64/bin/g++.exe",
      "args": [
        "-std=c++17",
        "-g",
        "-mconsole",
        "${workspaceFolder}/Lab5/main.cpp",
        "${workspaceFolder}/Lab5/Pokemon.cpp",
        "${workspaceFolder}/Lab5/Charmander.cpp",
        "-o",
        "${workspaceFolder}/Lab5/main.exe"
      ],
      "options": { "cwd": "${workspaceFolder}" },
      "problemMatcher": ["$gcc"],
      "group": { "kind": "build", "isDefault": true }
    }
  ]
}
